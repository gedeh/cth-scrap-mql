{
  "name": "cth-scrap-mql",
  "version": "1.0.0",
  "description": "Scrape MQL5 websites to collect account details",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "build": "npm-run-all build:*",
    "build:1": "rimraf dist",
    "build:2": "mkdirp dist",
    "build:3": "rollup index.mjs -o dist/built.js --format cjs",
    "build:4": "pkg --public dist/built.js --compress GZip --targets mac,win --output dist/mql-web-scraper"
  },
  "bin": "index.mjs",
  "pkg": {
    "targets": [
      "node14-win-x64"
    ],
    "options": [
      "experimental-modules"
    ]
  },
  "author": "gedehnok",
  "license": "MIT",
  "dependencies": {
    "csv-stringify": "^6.0.5",
    "puppeteer-core": "^13.5.2"
  },
  "devDependencies": {
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1"
  }
}
